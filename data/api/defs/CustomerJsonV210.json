    "mobile_phone_number",
    "customer_number",
    "highest_education_attained",
    "dob_of_dependants",
    "bank_id",
    "date_of_birth",
    "last_ok_date",
    "employment_status",
    "legal_name",
    "face_image",
    "dependants",
